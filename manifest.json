{
    "manifest_version": 2,
    "name": "PKULaw Search",
    "version": "1.0",
    "description": "A browser extension that searches for legal cases on PKULaw.",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "browser_action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "storage",
      "http://www.pkulaw.com/*"
    ]
  }